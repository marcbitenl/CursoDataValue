											"physicalType": "varchar"
										},
										"sink": {
											"name": "order_status",
											"type": "String",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "order_purchase_timestamp",
											"type": "DateTime",
											"physicalType": "datetime"
										},
										"sink": {
											"name": "order_purchase_timestamp",
											"type": "String",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "order_approved_at",
											"type": "DateTime",
											"physicalType": "datetime"
										},
										"sink": {
											"name": "order_approved_at",
											"type": "String",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "order_delivered_carrier_date",
											"type": "DateTime",
											"physicalType": "datetime"
										},
										"sink": {
											"name": "order_delivered_carrier_date",
											"type": "String",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "order_delivered_customer_date",
											"type": "DateTime",
											"physicalType": "datetime"
										},
										"sink": {
											"name": "order_delivered_customer_date",
											"type": "String",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "order_estimated_delivery_date",
											"type": "DateTime",
											"physicalType": "datetime"
										},
										"sink": {
											"name": "order_estimated_delivery_date",
											"type": "String",
											"physicalType": "String"
										}
									}
								],
								"typeConversion": true,
								"typeConversionSettings": {
									"allowDataTruncation": true,
									"treatBooleanAsNumber": false
								}
							}
						},
						"inputs": [
							{
								"referenceName": "AzureSqlTable1",
								"type": "DatasetReference",
								"parameters": {}
							}
						],
						"outputs": [
							{
								"referenceName": "DelimitedText1",
								"type": "DatasetReference",
								"parameters": {}
							}
						]
					},
