					"location": {
						"type": "AzureBlobStorageLocation",
						"fileName": "tb_olist_orders.csv",
						"folderPath": "landing-zone/sql-dvacademy/tb_olist_orders",
						"container": "dev"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/AzureBlobStorage1')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/ppl_dvacademy_aula2')]",
